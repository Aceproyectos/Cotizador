<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pisos</title>
    <script src="javascript/jquery-3.6.1.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="style/pisos.css">
</head>

<body>
    <nav class="navbar navbar-light bg-white">
        <div class="container">
            <a class="navbar-brand" href="#">
                <img src="/images/redes/LOGO-Acemar-Gris-2020 US.png"" alt="" width="auto" height="70">
            </a>
            <div class="lk-qtr p-2 rounded-pill">
                <a href="prueba" class="text-decoration-none text-dark">Quoter</a>
            </div>
        </div>
        <div class="mt-2 linea"></div>
    </nav>
    <table class="tabla">
        <tbody class="columnas">
          <% datos.forEach(function (datos) { %>
          <tr>
            <td>
              <input type="hidden" class="id" value="<%= datos.id%>" />
            </td>
            <td class="una">
              <section id="sec">
                <div class="articulo">
                  <a class="detalle" href="formulario/<%= datos.id%>">
                    <img
                      class="puerta"
                      src="images/productos/<%=datos.directorio%>/<%=datos.imgpuerta%>"
                    />
                    <h4
                      style="font-size: 18px; color: rgb(107, 105, 105); margin-bottom: 10px"
                      class="nom-prod"
                    >
                      <%= datos.producto%>
                    </h4>
                  </a>
                </div>
              </section>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
</body>

</html>