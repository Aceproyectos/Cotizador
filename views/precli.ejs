<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>precios</title>
</head>
<body>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>id</th>
                <th>usuario</th>
                <th>precios</th>
            </tr>
        </thead>
        <tbody>
            <% datos.forEach(function (datos) {%>
                <tr>
                    <td>
                        <input type="text" name="id" value="<%= datos.producto %>">
                    </td>
                    <td>
                        <%= datos.layer1%>
                    </td>
                    <td>
                        <%= datos.layer3%>
                    </td>
                    <td>
                        <input type="submit" value="precios">
                    </td>
                </tr>
                <%})%>
        </tbody>
    </table>
</body>
</html>