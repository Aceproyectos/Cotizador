<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style/estilos.css" />
    <link rel="stylesheet" href="style/guardados.css" />
</head>

<body>
    <div id="areaImprimir">
        <header>
            <ul>
                <div class="img-nav">
                    <img class="img-nav"
                        src="https://acemardistributor.com/wp-content/uploads/2022/07/LOGO-Acemar-Gris-2020.png" />
                </div>
                <ul class="nav justify-content-end quoter-ul">
                    <li class="nav-item ">
                        <a href="guardados" aria-current="page" class="Quoter-nav">Quoter</a>
                    </li>
                    <li>
                        <a class="text-black cerrarlog" style="color: black;" href="/">Logout</a>
                    </li>
                </ul>
            </ul>
        </header>
        <div class="linea"></div>
        <div class="titulo">
            <label class="arr"><b> DISTRIBUTORS QUOTATION</b></label>
        </div>
        <table>
            <thead>
                <th>#</th>
                <th>Producto</th>
                <th>Codigo</th>
                <th>Foto</th>
                <th>Frame</th>
                <th>Foto</th>
                <th>Height</th>
                <th>Width</th>
                <th>Finish</th>
                <th>Opening</th>
                <th>Core</th>
                <th>Thickness</th>
                <th>Quantity</th>
                <th>Subtotal</th>
                <th>Eliminar</th>
            </thead>
            <div>
                <tbody>
                    <% datos.forEach(function (datos) { %>

                        <tr>
                            <td>
                                <p class="contador">
                                    <input type="text" class="id" value="<%= datos.id%>">
                                </p>
                            </td>
                            <td>
                                <%= datos.producto%>
                            </td>
                            <td>
                                <%= datos.codigo%>
                            </td>
                            <td>
                                <input type="image" src="images/productos/<%= datos.imgpuerta%>" height="100px">
                            </td>
                            <td>
                                <%= datos.frame%>
                            </td>
                            <td>
                                <input type="image" src="images/productos/<%= datos.imgframe%>" height="70px">
                            </td>
                            <td>
                                <%= datos.height%>
                            </td>
                            <td>
                                <%= datos.width%>
                            </td>
                            <td>
                                <%= datos.finish%>
                            </td>
                            <td>
                                <%= datos.opening%>
                            </td>
                            <td>
                                <%= datos.core%>
                            </td>
                            <td>
                                <%= datos.thickness%>
                            </td>
                            <td>
                                <%= datos.quantity%>
                            </td>
                            <td>
                                <%= datos.total%>
                            </td>
                            <td>
                                <input type="submit" value="eliminar" class="elim" id="">
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </div>
        </table>
        <div class="conpacto">
            <div class="informacion">
                <label for="">2310 Tall Pines Drive - Suite 230, Largo Florida 33771</label>
                <br>
                <label for="">727 584 3711</label>
                <br>
                <label for="">sales@acemar.us</label>
            </div>
            <div class="tot">
            </div>
            <div class="ultimo">
                <b><label for="">subtotal:</label></b>
                <%= pagar[0].comprando%>
                    <input type="hidden" value="<%= pagar[0].comprando%>" class="total" id="total" name="">
                    <br>
                    <b><label for="">cantidad:</label></b>
                    <%= quantity[0].cantidad%>
                        <input type="hidden" value="<%= quantity[0].cantidad%>" class="cantidad" id="cantidad" name="">
                        <br>
                        <b><span>impuesto: </span></b><span>27%</span>
                        <br>
                        <b><label for="">flete:</label></b>
                        <span id="subtotal"></span>
                        <br>
                        <b><label for="">total:</label></b>
                        <span id="lblLenguajeSeleccionado"></span>
            </div>
        </div>
    </div>
    <input type="button" onclick="printDiv('areaImprimir')" value="imprimir div" />
</body>
<script src="javascript/jquery-3.6.1.min.js"></script>
